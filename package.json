    "workbox-webpack-plugin": "5.1.4"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development node scripts/start.js",
    "build": "cross-env NODE_ENV=production node scripts/build.js",
    "test": "cross-env NODE_ENV=test node scripts/test.js --watchAll=false",
    "test:dev": "cross-env NODE_ENV=test node scripts/test.js --coverage --reporters=default",
    "cy:open": "cross-env NODE_ENV=test cypress open --config-file cypress.json",
    "cy:open:local": "cross-env NODE_ENV=test cypress open --config-file cypress.json --config baseUrl=http://localhost:3000",
    "cy:headless": "cross-env NODE_ENV=test cypress run --config-file cypress-headless.json",
    "lint": "eslint 'src/**'",
    "postinstall": "link-module-alias && husky install",
    "preinstall": "command -v link-module-alias && link-module-alias clean || true",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable"
  },
  "license": "GPL-3.0",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "_moduleAliases": {
    "~root": "./src",
    "~app": "./src/app",
    "~lib": "./src/lib",
    "~config": "./config"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.ts"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.{js,jsx,ts,tsx}",
      "<rootDir>/src/**/*.{spec,test}.{js,jsx,ts,tsx}"
    ],
    "testRunner": "<rootDir>/node_modules/jest-circus/runner.js",
    "transform": {
      "^.+\\.(js|jsx|mjs|cjs|ts|tsx)$": "<rootDir>/config/jest/babelTransform.js",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|cjs|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs|cjs|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "modulePaths": [],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ],
    "resetMocks": true,
    "testEnvironment": "~config/jest/TestsEnvironments",
    "testTimeout": 10000
  },
  "devDependencies": {
    "@testing-library/dom": "^7.30.4",
    "@types/bs58check": "^2.1.0",
    "@types/enzyme": "^3.10.8",
    "@types/jest": "^24.0.0",
    "@types/jsdom": "^16.2.10",
    "@types/jsdom-global": "^3.0.2",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.0.2",
    "@types/randombytes": "^2.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.7",
    "@types/react-router-dom": "^5.1.7",
    "@types/styled-components": "^5.1.9",
    "@types/underscore": "^1.11.2",
    "@types/uuid": "^8.3.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "babel-plugin-inline-react-svg": "^2.0.1",
    "cypress": "^7.3.0",
    "cypress-file-upload": "^5.0.7",
    "enzyme": "^3.11.0",
    "enzyme-to-json": "^3.6.2",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "husky": "^6.0.0",
    "jest-environment-jsdom": "^26.6.2",
    "jest-environment-node": "^26.6.2",
    "jest-environment-uint8array": "^1.0.0",
    "jest-html-reporter": "^3.3.0",
    "jsdom": "^16.5.3",
    "jsdom-global": "^3.0.2",
    "pinst": "^2.1.6",
    "sass-lint": "^1.13.1",
    "ts-node": "^9.1.1"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
