  "scripts": {
    "prettify": "prettier --write --log-level silent src/**/*.{ts,tsx,js,jsx,json,css,scss,md}",
    "type-check": "tsc --noEmit",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "dev": "rm -rf node_modules/.vite/ && vite",
    "start": "vite",
    "build": "node --max_old_space_size=16384 ./node_modules/vite/bin/vite.js build",
    "cy:open": "yarn local-node && cross-env NODE_ENV=test cypress open --config-file cypress.json",
    "cy:open:local": "yarn local-node && cross-env NODE_ENV=test cypress open --config-file cypress.json --config baseUrl=http://localhost:3000",
    "cy:headless": "cross-env NODE_ENV=test cypress run --config-file cypress-headless.json",
    "lint": "eslint src/**",
    "postinstall": "husky install",
    "prepublishOnly": "pinst --disable",
    "postpublish": "pinst --enable",
    "prod:watch": "yarn build && serve -s build -l 5173",
    "serve": "serve -s build -l 5173",
    "lint-staged": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,json,css,scss,md}": [
      "prettier --write --log-level silent",
      "git add"
    ]
  },
